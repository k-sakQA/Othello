{
  "default_browser": "chromium",
  "timeout_seconds": 30,
  "max_iterations": 10,
  "paths": {
    "test_instructions": "test-instructions",
    "test_results": "test-results",
    "logs": "logs",
    "results": "results",
    "reports": "reports"
  },
  "playwright_agent": {
    "enabled": true,
    "agent_type": "planner",
    "generate_code": false
  },
  "claude_api": {
    "enabled": false,
    "model": "claude-3-5-sonnet-20241022",
    "max_tokens": 4000
  },
  "coverage_threshold": {
    "target_percentage": 80,
    "min_iterations": 3,
    "max_iterations_without_improvement": 2
  },
  "target_systems": []
}
